local player = game:GetService("Players").LocalPlayer
local inventory = player.PlayerGui.Inventory.Frame.Main.Pets.Normal

-- Replace "rbxassetid://your_asset_idhere" with the asset ID of your custom Roblox asset
local customAssetId = "rbxassetid://12089951494"

while true do
    for _, petIcon in ipairs(inventory:GetDescendants()) do
        if petIcon.Name == "PetIcon" then
            petIcon.Image = customAssetId
        end 
    end
    
    for _, petIcon in ipairs(inventory:GetDescendants()) do
        if petIcon.Name == "Level" and petIcon:IsA("TextLabel") then
            petIcon.Text = "????"
        end
    end
    
    wait()
end
